<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>showAllServlet</servlet-name>
        <servlet-class>servlet.showAllServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>findServlet</servlet-name>
        <servlet-class>servlet.findServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admloginServlet</servlet-name>
        <servlet-class>servlet.admloginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stdloginServlet</servlet-name>
        <servlet-class>servlet.stdloginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>servlet.registerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stdmodifyServlet</servlet-name>
        <servlet-class>servlet.stdmodifyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addgradeServlet</servlet-name>
        <servlet-class>servlet.addgradeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>findbyStdNumServlet</servlet-name>
        <servlet-class>servlet.findbyStdNumServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletebyStdNumServlet</servlet-name>
        <servlet-class>servlet.deletebyStdNumServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteAllServlet</servlet-name>
        <servlet-class>servlet.deleteAllServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admAddServlet</servlet-name>
        <servlet-class>servlet.admAddServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>showAllServlet</servlet-name>
        <url-pattern>/show</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>findServlet</servlet-name>
        <url-pattern>/find</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admloginServlet</servlet-name>
        <url-pattern>/admlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stdloginServlet</servlet-name>
        <url-pattern>/stdlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stdmodifyServlet</servlet-name>
        <url-pattern>/stdmodify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addgradeServlet</servlet-name>
        <url-pattern>/addgrade</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>findbyStdNumServlet</servlet-name>
        <url-pattern>/findbyStdNum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletebyStdNumServlet</servlet-name>
        <url-pattern>/deletebyStdNum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteAllServlet</servlet-name>
        <url-pattern>/deleteAll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admAddServlet</servlet-name>
        <url-pattern>/admAdd</url-pattern>
    </servlet-mapping>
    

    <welcome-file-list>
        <welcome-file>/initface.html</welcome-file>
    </welcome-file-list>
</web-app>